	"authors": [
		"maDU59_"
	],
	"contact": {
		"homepage": "https://madu59.github.io/",
		"sources": "https://github.com/maDU59/ProjectilesTrajectoryPreview"
	},
	"license": "MIT",
	"icon": "assets/ptp/icon.png",
	"environment": "*",
	"entrypoints": {
